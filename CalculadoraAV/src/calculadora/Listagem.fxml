<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import calculadora.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculadora.ListagemController">
   <children>
      <TableView fx:id="tabela" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: BLUE;">
        <columns>
          <TableColumn text="id">
            <cellValueFactory><PropertyValueFactory property="id" />
            </cellValueFactory>
         </TableColumn>
         <TableColumn text="Num1">
            <cellValueFactory><PropertyValueFactory property="firstN" />
            </cellValueFactory>
         </TableColumn>
         <TableColumn text="Num2">
            <cellValueFactory><PropertyValueFactory property="secondN" />
            </cellValueFactory>
         </TableColumn>
         <TableColumn text="Operador">
            <cellValueFactory><PropertyValueFactory property="operator" />
            </cellValueFactory>
         </TableColumn>
         <TableColumn text="result">
            <cellValueFactory><PropertyValueFactory property="result" />
            </cellValueFactory>
         </TableColumn>
        </columns>
      </TableView>
      
    
   </children>
</AnchorPane>
